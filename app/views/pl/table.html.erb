  <%= image_tag 'spacer.gif', :size => '1x1' %>
  <div  class="container">
    <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th>Pos</th>
        <th>Team</th>
        <th>P</th>
        <th>W</th>
        <th>D</th>
        <th>L</th>
        <th>F</th>
        <th>A</th>
        <th>GD</th>
        <th>Pts</th>
      </tr>
    </thead>
    <tbody>
      <% for num in 0..@table.length - 1 do %>
      <tr>
        <td><%= num + 1 %></td>
        <td><%= @table[num].team %></td>
        <td><%= @table[num].play %></td>
        <td><%= @table[num].win %></td>
        <td><%= @table[num].draw %></td>
        <td><%= @table[num].lose %></td>
        <td><%= @table[num].get_score %></td>
        <td><%= @table[num].cns_score %></td>
        <td><%= @table[num].goal_diff %></td>
        <td><%= @table[num].points %></td>
      </tr>        
      <% end %>
    </tbody>
    </table>
  </div>

